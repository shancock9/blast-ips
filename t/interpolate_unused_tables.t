#!/usr/bin/perl 
use strict;
use warnings;
use Test;
use Blast::IPS;

# Here we check interpolation in gamma using some tables which are not included in the
# database for Blast::IPS

# Tables for gamma=1.65 were calculated with 8000 points and are
# accurate to about 1.e-6. They were not included in the final collection of
# shock tables because they are too close to gamma=1.667, but we can use them
# to check the interpolation scheme.

# A 4000 point cylindrical calculation at gamma=1.51 was computed but not used
# in the collection so it is included here to test gamma interpolation.

# A cylindrical calculation at gamma=1.5 had some undesirable initial
# oscillations and was replaced in the collection with a calculation at
# gamma=1.49.  This table is included below but it still has a very large error
# (>1.e-5) so it is not currently used.  I'm not sure what is wrong with this
# calculation and table.  The initial values may not have been correctly set.


my $rtables;
my $rinfo;

BEGIN {

    # This is the index to the tables; the data is below.
    #   table_name => [symmetry, gamma, ...]
    $rinfo = {
        'S1.65' => [ 2, 1.65, 7.53e-07, 8000, 2.13e-07, 10,  1.24e-07, 1.29e-07, 5e-07, 1749.1, -0.70001, ],
        'C1.65' => [ 1, 1.65, 6.93e-07, 8000, 2.2e-07,  50,  1.3e-07,  6.31e-08, 5e-07, 87.836, -1.275, ],
        'P1.65' => [ 0, 1.65, 1.45e-06, 8000, 5.3e-07,  100, 3.74e-07, 5.8e-07,  5e-07,,, ],
        #'C1.5'  => [ 1, 1.5,  1.23e-06, 8000, 2.1e-07,  50,  4.76e-07, 2.5e-07,  5e-07, 77.778, -1.1605, ],
        'C1.51' => [ 1, 1.51, 1.18e-06, 4000, 8.29e-07, 30,  5.58e-07, 1.21e-07, 5e-07, 78.316, -1.1671, ],
    };

    my $num = scalar keys %{$rinfo};
    plan tests => $num;

}

my $tol = 2.5e-6;

# loop over all tables
my $verbose=0;
foreach my $key ( keys %{$rinfo} ) {
    my $dY_max = 0;
    my $item   = $rinfo->{$key};
    my ( $symmetry, $gamma ) = @{$item};

    print STDERR "sym=$symmetry, gamma=$gamma\n" if ($verbose);

    # make a table interpolated to this gamma
    my $blast_table = Blast::IPS->new( symmetry => $symmetry, gamma => $gamma );

    # loop to compare the interpolated table with the actual table at each range....
    # but do not compare beyond the bounds of the interpolated table because we do not
    # want to compare with extrapolated values.
    my $rtab    = $rtables->{$key};
    my $rbounds = $blast_table->get_table_bounds();
    my $Xmin    = $rbounds->[0]->[0];
    my $Xmax    = $rbounds->[1]->[0];
    foreach my $entry ( @{$rtab} ) {
        my ( $X, $Y, $dYdX, $Z, $dZdX ) = @{$entry};
        my ( $X_i, $Y_i, $dYdX_i, $Z_i, $dZdX_i ) =
          @{ $blast_table->wavefront( 'X' => $X )->{'TableVars'} };
        my $alpha_1 = $blast_table->get_alpha();
        my $alpha_2 = Blast::IPS::alpha_interpolate( $symmetry, $gamma );
        my $dalpha  = $alpha_2 - $alpha_1;
        my $adY     = abs( $Y - $Y_i );

	print STDERR "$X\t$Y\t$Y_i\t$adY\n" if ($verbose && $verbose>1);

        # Important: only compare table entries; do not look at extrapolations
        if ( $X >= $Xmin && $X <= $Xmax ) {
            if ( $adY > $dY_max ) { $dY_max = $adY }
        }
    }
    ok( $dY_max < $tol ) || print STDERR "symmetry=$symmetry, gamma=$gamma, dY_max=$dY_max\n";
}

